{
    "name": "extbasebook/simpleblog",
    "type": "typo3-cms-extension",
    "description": "Demo EXtension",
    "authors": [
        {
            "name": "MalikDon01",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ExtbaseBook\\Simpleblog\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExtbaseBook\\Simpleblog\\Tests\\": "Tests"
        }
    },
    "replace": {
        "simpleblog": "self.version",
        "typo3-ter/simpleblog": "self.version"
    }
}